{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "styleguide#build": {
      "dependsOn": [
        "^build",
        "$SERVER"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": [
        "build"
      ]
    }
  },
  "globalDependencies": [
    ".env.*"
  ]
}
